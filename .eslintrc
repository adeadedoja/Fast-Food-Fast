{
    "env": {
    "node": true,
    "es6": true,
    "browser": true,
    "jest": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "array-callback-return":"off",
        "class-methods-use-this":"off",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "linebreak-style": "off",
        "no-console":"off",
        "no-else-return":"off",
        "no-undef": 1,
        "no-empty": "off",
        "prefer-destructuring":"off",
        "no-loop-func" :"off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}