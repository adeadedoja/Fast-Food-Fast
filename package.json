{
  "name": "fast-food-fast",
  "version": "1.0.0",
  "description": "A food delivery service application for a restaurant, to enable a restaurant manage customer orders better and deliver fast food items to customers in a 'Split Second",
  "main": "app.js",
  "scripts": {
    "build": "babel src -d lib",
    "migrate": "npx babel-node ./app/migrate.js ",
    "pretest": "npx babel-node ./app/migrate.js ",
    "start": "npm run migrate && nodemon ./app/app.js --exec babel-node --",
    "start-prod": "nodemon ./app/app.js --exec babel-node --",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec --compilers js:babel-core/register ./app/tests/**/*.test.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc --reporter=html --reporter=text --reporter=lcov mocha --compilers js:babel-core/register ./app/tests/**/*.test.js --timeout 10000 --exit"
  },
  "engines": {
    "node": "8.9.3",
    "npm": "5.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oluwajuwon/Fast-Food-Fast.git"
  },
  "keywords": [
    "order",
    "food",
    "delivery",
    "restaurant"
  ],
  "author": "Oluwajuwon Fagbohungbe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oluwajuwon/Fast-Food-Fast/issues"
  },
  "homepage": "https://github.com/oluwajuwon/Fast-Food-Fast#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "pg": "^7.4.3",
    "supertest": "^3.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1"
  }
}
